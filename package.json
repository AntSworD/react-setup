{
  "name": "react-setup",
  "description": "A Universal React setup with i18n: Babel 6, Koa 2, React, React Router, React Transmit, React Bootstrap, React-intl, Mocha, Webpack HMR, PostCSS, ESLint.",
  "version": "2.0.8",
  "author": "Duc Ng (https://github.com/ngduc)",
  "license": "MIT",
  "homepage": "https://github.com/ngduc/react-setup",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngduc/react-setup.git"
  },
  "bugs": {
    "url": "https://github.com/ngduc/react-setup/issues"
  },
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "starter",
    "boilerplate",
    "template",
    "webpack",
    "koa",
    "transmit",
    "boostrap",
    "i18n",
    "intl",
    "postcss"
  ],
  "main": "dist/server.js",
  "scripts": {
    "start": "node node_modules/forever/bin/forever --minUptime 1000 --spinSleepTime 1000 -c \"node --harmony\" ./dist/server.js",
    "build:msg": "babel --plugins react-intl src/**/messages.js && babel-node tools/scripts/translate.js",
    "build-server": "node node_modules/webpack/bin/webpack --colors --display-error-details --config configs/webpack.server.js",
    "build-client": "node node_modules/webpack/bin/webpack --colors --display-error-details --config configs/webpack.client.js",
    "build": "rm -rf ./dist && rm -rf ./static && node node_modules/concurrently/src/main \"npm run build-server\" \"npm run build-client\" \"npm run build:msg\"",
    "watch-server": "node node_modules/webpack/bin/webpack --watch --verbose --colors --display-error-details --config configs/webpack.server-watch.js",
    "watch-server-start": "node node_modules/just-wait --pattern \"dist/*.js\" && npm run start",
    "watch-client": "node node_modules/webpack-dev-server/bin/webpack-dev-server --config configs/webpack.client-watch.js",
    "dev": "rm -rf ./dist && rm -rf ./static && node node_modules/concurrently/src/main --kill-others \"npm run watch-server-start\" \"npm run watch-server\" \"npm run watch-client\"",
    "test": "NODE_PATH=./src mocha --require ./tools/test/test-init --compilers js:babel-core/register ./src/**/*.spec.js --recursive"
  },
  "dependencies": {
    "babel-polyfill": "6.6.1",
    "fetch-plus": "3.8.1",
    "fetch-plus-bearerauth": "3.5.0",
    "fetch-plus-json": "3.6.0",
    "intl": "^1.1.0",
    "intl-locales-supported": "^1.0.0",
    "isomorphic-fetch": "2.2.1",
    "koa": "^2.0.0-alpha.3",
    "koa-bodyparser": "^3.0",
    "koa-compose": "^3.0.0",
    "koa-compress": "^2.0.0",
    "koa-convert": "^1.2",
    "koa-generic-session": "^1.10",
    "koa-passport": "^2.0",
    "koa-router": "7.0.1",
    "koa-serve-static": "^1.0.0",
    "mustache": "^2.2.1",
    "react": "0.14.7",
    "react-bootstrap": "^0.28.3",
    "react-dom": "0.14.7",
    "react-inline-css": "2.1.0",
    "react-intl": "2.0.0-rc-1",
    "react-router": "2.0.1",
    "react-transmit": "https://github.com/ngduc/react-transmit.git",
    "serialize-javascript": "^1.2.0"
  },
  "devDependencies": {
    "autoprefixer": "6.3.3",
    "babel": "6.5.2",
    "babel-cli": "^6.6.5",
    "babel-core": "6.7.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-intl": "2.1.1",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "bootstrap": "^3.3.6",
    "chai": "^3.5.0",
    "classnames": "^2.2.3",
    "concurrently": "2.0.0",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "forever": "0.15.1",
    "glob": "^7.0.3",
    "html-webpack-plugin": "^2.9.0",
    "intl-messageformat-parser": "^1.2.0",
    "isparta": "^4.0.0",
    "jsdom": "^8.1.0",
    "json-loader": "0.5.4",
    "just-wait": "1.0.5",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "8.0.2",
    "postcss-loader": "^0.8.1",
    "postcss-mixins": "4.0.1",
    "postcss-modules-values": "1.1.1",
    "postcss-nested": "1.0.0",
    "postcss-simple-vars": "1.2.0",
    "react-addons-css-transition-group": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "teaspoon": "^6.1.2",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1",
    "webpack-node-externals": "1.0.0"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}
